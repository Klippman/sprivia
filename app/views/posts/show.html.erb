
<p> Post: <%= @post.entry %></p>
<p> Posted by: <%= @post.user.name %></p>

<% @post.comments.each do |c| %>
<%= c.body %>
<% end %>


<%= link_to("Edit Post", edit_post_path(@post)) %>
</br>
<%= link_to("Delete Post", @post, method: 'delete') %>
</br>
<%= link_to("Add Comment", new_comment_path(post_id: @post.id)) %>
